{
  "name": "curlconverter",
  "displayName": "curlconverter",
  "description": "Paste curl commands as Python, JavaScript, PHP, R, Go, Java or Rust.",
  "version": "0.0.3",
  "publisher": "curlconverter",
  "icon": "icon.png",
  "homepage": "https://curlconverter.com/",
  "author": {
    "name": "Boris Verkhovskiy",
    "email": "boris.verk@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/curlconverter/curlconverter-vscode"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:curlconverter.fromClipboard",
    "onLanguage:go",
    "onLanguage:java",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact",
    "onLanguage:json",
    "onLanguage:jsonc",
    "onLanguage:php",
    "onLanguage:python",
    "onLanguage:r",
    "onLanguage:rust",
    "onLanguage:yaml",
    "onLanguage:dart",
    "onLanguage:elixir",
    "onLanguage:matlab"
