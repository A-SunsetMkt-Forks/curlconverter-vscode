{
  "name": "curlconverter",
  "version": "0.0.1",
  "displayName": "curlconverter",
  "description": "VS Code extension to convert curl commands to Python, JS and other languages",
  "publisher": "curlconverter",
  "icon": "icon.png",
  "homepage": "https://github.com/curlconverter/curlconverter",
  "author": {
    "name": "Boris Verkhovskiy",
    "email": "boris.verk@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/curlconverter/vs-code"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:curlconverter.fromClipboard",
    "onLanguage:go",
    "onLanguage:java",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact",
    "onLanguage:json",
    "onLanguage:jsonc",
    "onLanguage:php",
    "onLanguage:python",
    "onLanguage:r",
    "onLanguage:rust",
    "onLanguage:yaml",
    "onLanguage:dart",
    "onLanguage:elixir",
    "onLanguage:matlab"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "curlconverter.fromClipboard",
        "title": "Paste cURL as code"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "curlconverter.fromClipboard",
          "title": "Paste cURL as code",
          "group": "9_cutcopypaste@5",
          "when": "editorLangId in curlconverter.supportedLanguages"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "curlconverter": "^3.21.0"
  }
}
