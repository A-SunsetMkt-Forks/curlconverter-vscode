      {
        "command": "curlconverter.toJavaShort",
        "title": "Java",
        "category": "curlconverter"
      },
      {
        "command": "curlconverter.toBrowserShort",
        "title": "JavaScript",
        "category": "curlconverter"
      },
      {
        "command": "curlconverter.toJsonStringShort",
        "title": "JSON",
