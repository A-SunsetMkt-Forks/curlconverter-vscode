        },
        {
          "command": "curlconverter.toBrowser",
          "title": "Paste cURL As JavaScript",
          "group": "9_cutcopypaste@5",
          "when": "editorLangId == javascript"
        },
        {
          "command": "curlconverter.toBrowser",
          "title": "Paste cURL As JavaScript",
          "group": "9_cutcopypaste@5",
          "when": "editorLangId == typescript"
        },
        {
          "command": "curlconverter.toBrowser",
          "title": "Paste cURL As JavaScript",
          "group": "9_cutcopypaste@5",
